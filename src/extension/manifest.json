{
  "manifest_version": 3,
  "name": "TIPS",
  "version": "0.1.0",
  "description": "TIPS: AI inference powered by Anthropic",
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
    "activeTab",
    "storage"
  ]
}
